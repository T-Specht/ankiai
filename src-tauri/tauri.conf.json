{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "copyright": "Tim Specht",
    "publisher": "Tim Specht"
  },
  "productName": "AnkiAI",
  "mainBinaryName": "AnkiAI",
  "version": "../package.json",
  "identifier": "io.specht.ankiai",
  "plugins": {},
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "AnkiAI",
        "width": 800,
        "height": 600,
        "minHeight": 200,
        "minWidth": 300,
        "useHttpsScheme": true
      }
    ]
  }
}